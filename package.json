{
  "name": "entria-fullstack-challenge",
  "version": "1.0.0",
  "description": "Full-stack application for Entria Challenge, using Relay Modern and GraphQL.",
  "main": "none ",
  "private": true,
  "scripts": {
    "start": "lerna run start --stream",
    "start:web": "lerna run start --scope /packages/web --stream",
    "start:server": "lerna run start --scope /packages/server --stream",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mateusmforte/entria-fullstack-challenge.git"
  },
  "keywords": [
    "React",
    "NodeJS",
    "JavaScript",
    "TypeScript",
    "GraphQL",
    "MongoDB",
    "Relay",
    "Modern",
    "Mongoose",
    "KoaJs"
  ],
  "author": "Mateus Moura Forte",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mateusmforte/entria-fullstack-challenge/issues"
  },
  "homepage": "https://github.com/mateusmforte/entria-fullstack-challenge#readme",
  "devDependencies": {
    "get-graphql-schema": "^2.1.2",
    "global": "^4.3.2",
    "lerna": "^3.13.3"
  }
}
